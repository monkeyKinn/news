{
  "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/tabbar/index/index",
      "style": {
        // 取消默认顶部导航栏,custom 定制的
        "navigationStyle": "custom",
        // 状态栏颜色
        "navigationBarTextStyle": "white",
        "navigationBarTitleText": "首页pagestext"
      }
    }, {
      "path": "pages/tabbar/follow/follow",
      "style": {
        "navigationBarTitleText": "关注pagesText",
        "enablePullDownRefresh": false
      }

    }, {
      "path": "pages/tabbar/mine/mine",
      "style": {
        "navigationBarTitleText": "我的pagesText",
        "enablePullDownRefresh": false
      }

    }, {
      "path": "pages/home-search/home-search",
      "style": {
        // 取消默认顶部导航栏,custom 定制的
        "navigationStyle": "custom"
      }

    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#666",
    "selectedColor": "#F07373",
    "backgroundColor": "#FFF",
    "list": [{
        "pagePath": "pages/tabbar/index/index",
        "iconPath": "static/home.png",
        "selectedIconPath": "static/home-active.png",
        "text": "首页"
      }, {
        "pagePath": "pages/tabbar/follow/follow",
        "iconPath": "static/follow.png",
        "selectedIconPath": "static/follow-active.png",
        "text": "关注"
      }, {
        "pagePath": "pages/tabbar/mine/mine",
        "iconPath": "static/my.png",
        "selectedIconPath": "static/my-active.png",
        "text": "我的"
      }

    ]
  }
}
